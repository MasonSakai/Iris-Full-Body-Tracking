{
    "name": "remote-one-camera-processing",
    "version": "0.0.1",
    "description": "Demo for pose detection api",
    "private": true,
    "engines": {
        "node": ">=8.9.0"
    },
    "dependencies": {
        "@tensorflow-models/pose-detection": "^2.1.3",
        "npm-flatten": "^0.0.1",
        "string-similarity": "^4.0.4"
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "tsc --build && npx webpack",
        "clean": "tsc --build --clean",
        "lint": "eslint . --ext .ts",
        "build-dep": "cd ../../ && yarn && yarn build",
        "link-core": "yalc link @tensorflow/tfjs-core",
        "link-webgl": "yalc link @tensorflow/tfjs-backend-webgl"
    },
    "browser": {
        "crypto": false
    },
    "devDependencies": {
        "@types/node": "^18.19.50",
        "eslint": "^9.27.0",
        "globals": "^16.2.0",
        "open": "^9.1.0",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.32.1",
        "webpack": "^5.97.1",
        "webpack-cli": "^5.1.4"
    },
    "resolutions": {
        "is-svg": "4.3.1"
    },
    "eslintConfig": {
        "extends": "google",
        "rules": {
            "require-jsdoc": 0,
            "valid-jsdoc": 0,
            "@typescript-eslint/no-require-imports": "error"
        },
        "env": {
            "es6": true
        },
        "parserOptions": {
            "ecmaVersion": 8,
            "sourceType": "module"
        }
    },
    "eslintIgnore": [
        "dist/"
    ]
}
